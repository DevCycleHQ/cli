{
  "name": "@devcycle/cli",
  "version": "4.2.11",
  "description": "DevCycle CLI Tool",
  "author": "support@devcycle.com",
  "bin": {
    "dvc": "./bin/run"
  },
  "homepage": "https://github.com/DevCycleHQ/cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "DevCycleHQ/cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@babel/parser": "^7.18.9",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@types/estraverse": "^5.1.1",
    "@types/inquirer": "^8.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/validator": "^13.7.1",
    "axios": "^0.24.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-ux": "^6.0.8",
    "estraverse": "^5.3.0",
    "inquirer": "^8.2.0",
    "js-sha256": "^0.9.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^5.0.1",
    "node-emoji": "^1.11.0",
    "parse-diff": "^0.9.0",
    "recast": "^0.21.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/node-emoji": "^1.8.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "nock": "^13.2.2",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "dvc",
    "dirname": "devcycle",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "login": {
        "description": "Log in to DevCycle"
      },
      "features": {
        "description": "Access or modify Features with the Management API"
      },
      "variables": {
        "description": "Access or modify Variables with the Management API"
      },
      "projects": {
        "description": "Access Projects with the Management API"
      },
      "org": {
        "description": "Switch organizations"
      },
      "alias": {
        "description": "Manage repository variable aliases"
      },
      "repo": {
        "description": "Manage repository configuration"
      },
      "status": {
        "description": "Check CLI status"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && oclif manifest && oclif readme --multi",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme --multi",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme --multi && git add README.md"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "bugs": "https://github.com/DevCycleHQ/cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}